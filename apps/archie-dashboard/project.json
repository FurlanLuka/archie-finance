{
  "root": "apps/archie-dashboard",
  "sourceRoot": "apps/archie-dashboard/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/archie-dashboard",
        "index": "apps/archie-dashboard/src/index.html",
        "baseHref": "/",
        "main": "apps/archie-dashboard/src/main.tsx",
        "polyfills": "apps/archie-dashboard/src/polyfills.ts",
        "tsConfig": "apps/archie-dashboard/tsconfig.app.json",
        "assets": [
          "apps/archie-dashboard/src/favicon.ico",
          "apps/archie-dashboard/src/assets"
        ],
        "styles": ["apps/archie-dashboard/src/styles.css"],
        "scripts": [],
        "webpackConfig": "apps/archie-dashboard/webpack.config.js"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/archie-dashboard/src/environments/environment.ts",
              "with": "apps/archie-dashboard/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "archie-dashboard:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "archie-dashboard:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/archie-dashboard/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/archie-dashboard"],
      "options": {
        "jestConfig": "apps/archie-dashboard/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["dashboard"]
}
