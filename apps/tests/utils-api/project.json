{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/tests/utils-api/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/tests/utils-api/src",
        "main": "apps/tests/utils-api/src/main.ts",
        "tsConfig": "apps/tests/utils-api/tsconfig.app.json",
        "assets": ["apps/tests/utils-api/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/tests/utils-api/src/environments/environment.ts",
              "with": "apps/tests/utils-api/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "tests-utils-api:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/tests/utils-api/**/*.ts"]
      }
    },
    "build-docker": {
      "executor": "@archie/plugin/deployment:docker-build"
    }
  },
  "tags": []
}
