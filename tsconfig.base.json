{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noPropertyAccessFromIndexSignature": false,
    "baseUrl": ".",
    "paths": {
      "@archie-webapps/archie-dashboard/feature-history": ["libs/archie-dashboard/feature-history/src/index.ts"],
      "@archie-webapps/archie-dashboard/feature-onboarding": ["libs/archie-dashboard/feature-onboarding/src/index.ts"],
      "@archie-webapps/archie-dashboard/feature-rewards": ["libs/archie-dashboard/feature-rewards/src/index.ts"],
      "@archie-webapps/archie-dashboard/feature-settings": ["libs/archie-dashboard/feature-settings/src/index.ts"],
      "@archie-webapps/archie-dashboard/feature-wallet-and-collateral": [
        "libs/archie-dashboard/feature-wallet-and-collateral/src/index.ts"
      ],
      "@archie-webapps/shared/data-access-archie-api/*": ["libs/shared/data-access-archie-api/src/*"],
      "@archie-webapps/shared/data-access-session": ["libs/shared/data-access-session/src/index.ts"],
      "@archie-webapps/shared/ui-design-system": ["libs/shared/ui-design-system/src/index.ts"],
      "@archie-webapps/shared/ui-theme": ["libs/shared/ui-theme/src/index.ts"],
      "@archie-webapps/ui-icons": ["libs/ui-icons/src/index.ts"],
      "@archie-webapps/util-constants": ["libs/util-constants/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
