{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noPropertyAccessFromIndexSignature": false,
    "baseUrl": ".",
    "paths": {
      "@archie-webapps/archie-dashboard/components": ["libs/archie-dashboard/components/src/index.ts"],
      "@archie-webapps/archie-dashboard/constants": ["libs/archie-dashboard/constants/src/index.ts"],
      "@archie-webapps/archie-dashboard/feature/collateral": ["libs/archie-dashboard/feature/collateral/src/index.ts"],
      "@archie-webapps/archie-dashboard/feature/collateralization": [
        "libs/archie-dashboard/feature/collateralization/src/index.ts"
      ],
      "@archie-webapps/archie-dashboard/feature/history": ["libs/archie-dashboard/feature/history/src/index.ts"],
      "@archie-webapps/archie-dashboard/feature/home": ["libs/archie-dashboard/feature/home/src/index.ts"],
      "@archie-webapps/archie-dashboard/feature/make-payment": [
        "libs/archie-dashboard/feature/make-payment/src/index.ts"
      ],
      "@archie-webapps/archie-dashboard/feature/onboarding": ["libs/archie-dashboard/feature/onboarding/src/index.ts"],
      "@archie-webapps/archie-dashboard/feature/payments": ["libs/archie-dashboard/feature/payments/src/index.ts"],
      "@archie-webapps/archie-dashboard/feature/rewards": ["libs/archie-dashboard/feature/rewards/src/index.ts"],
      "@archie-webapps/archie-dashboard/feature/settings": ["libs/archie-dashboard/feature/settings/src/index.ts"],
      "@archie-webapps/archie-dashboard/feature/withdraw": ["libs/archie-dashboard/feature/withdraw/src/index.ts"],
      "@archie-webapps/archie-dashboard/hooks": ["libs/archie-dashboard/hooks/src/index.ts"],
      "@archie-webapps/archie-dashboard/utils": ["libs/archie-dashboard/utils/src/index.ts"],
      "@archie-webapps/shared/constants": ["libs/shared/constants/src/index.ts"],
      "@archie-webapps/shared/data-access/archie-api/*": ["libs/shared/data-access/archie-api/src/*"],
      "@archie-webapps/shared/data-access/session": ["libs/shared/data-access/session/src/index.ts"],
      "@archie-webapps/shared/ui/design-system": ["libs/shared/ui/design-system/src/index.ts"],
      "@archie-webapps/shared/ui/icons": ["libs/shared/ui/icons/src/index.ts"],
      "@archie-webapps/shared/ui/theme": ["libs/shared/ui/theme/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
