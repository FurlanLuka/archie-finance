diff --git a/libs/plugin-deployment/src/executors/docker-build/executor.ts b/libs/plugin-deployment/src/executors/docker-build/executor.ts
new file mode 100644
index 0000000..9eacd46
--- /dev/null
+++ b/libs/plugin-deployment/src/executors/docker-build/executor.ts
@@ -0,0 +1,34 @@
+import { ExecutorContext, logger } from '@nrwl/devkit';
+import { execSync } from 'child_process';
+import { DockerBuildExecutorSchema } from './schema';
+
+export default async function runExecutor(
+  options: DockerBuildExecutorSchema,
+  context: ExecutorContext,
+) {
+  if (!context.projectName) {
+    logger.error(`Project name is not available.`);
+
+    return { success: false };
+  }
+
+  const projectConfig = context.workspace.projects[context.projectName];
+  const versionTag = `${projectConfig.name}-${options.imageTag}`;
+
+  logger.info(`Building docker image for ${projectConfig.name}...`);
+
+  execSync(
+    `docker build -f ${projectConfig.root}/Dockerfile.ci -t ${options.ecrRegistry}/${options.ecrRepository}:${versionTag} .`,
+  );
+
+  logger.info(`Pushing docker image to registry...`);
+
+  execSync(`
+    docker push ${options.ecrRegistry}/${options.ecrRepository}:${versionTag}
+  `);
+
+  console.log('Executor ran for Build', options);
+  return {
+    success: true,
+  };
+}
