{
  "$schema": "http://json-schema.org/schema",
  "name": "plugin-workspace-generator",
  "version": "0.0.1",
  "generators": {
    "deployment-config-generator": {
      "factory": "./src/generators/deployment-config-generator/generator",
      "schema": "./src/generators/deployment-config-generator/schema.json",
      "description": "deployment config generator"
    },
    "microservice-generator": {
      "factory": "./src/generators/microservice-generator/generator",
      "schema": "./src/generators/microservice-generator/schema.json",
      "description": "microservice generator"
    },
    "microservice-module-generator": {
      "factory": "./src/generators/microservice-module-generator/generator",
      "schema": "./src/generators/microservice-module-generator/schema.json",
      "description": "microservice module generator"
    },
    "microservice-project-target-generator": {
      "factory": "./src/generators/microservice-project-target-generator/generator",
      "schema": "./src/generators/microservice-project-target-generator/schema.json",
      "description": "microservice project target generator"
    }
  }
}
